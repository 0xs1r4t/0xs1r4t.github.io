{
    "name": "sirat-git",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "homepage": "https://git.sirat.xyz",
    "dependencies": {
       "@testing-library/jest-dom": "^5.11.4",
       "@testing-library/react": "^11.1.0",
       "@testing-library/user-event": "^12.1.10",
       "gh-pages": "^3.1.0",
       "react": "^17.0.2",
       "react-dom": "^17.0.2",
       "react-scripts": "4.0.3",
       "styled-components": "^5.2.1",
       "web-vitals": "^1.0.1"
    },
    "scripts": {
       "start": "react-scripts start",
       "predeploy": "npm run build",
       "deploy": "gh-pages -b main -d build",
       "build": "react-scripts build",
       "test": "react-scripts test",
       "eject": "react-scripts eject"
    },
    "eslintConfig": {
       "extends": [
          "react-app",
          "react-app/jest"
       ]
    },
    "browserslist": {
       "production": [
          ">0.2%",
          "not dead",
          "not op_mini all"
       ],
       "development": [
          "last 1 chrome version",
          "last 1 firefox version",
          "last 1 safari version"
       ]
    },
    "devDependencies": {
       "babel-plugin-styled-components": "^1.12.0",
       "prettier": "^2.2.1"
    },
    "prettier": {
       "singleQuote": false,
       "printWidth": 100,
       "semi": true,
       "bracketSpacing": true,
       "jsxBracketSameLine": true,
       "arrowParens": "avoid",
       "trailingComma": "all",
       "tabWidth": 3
    },
    "babel": {
       "presets": [
          "react-app"
       ],
       "plugins": [
          [
             "babel-plugin-styled-components",
             {
                "pure": true,
                "displayName": true,
                "ssr": true
             }
          ]
       ]
    }
 }